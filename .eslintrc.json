{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            ["@root", "./src"],
            ["@models", "./src/models"],
            ["@handlers", "./src/handlers"]
          ]
        }
      }
    }
}
